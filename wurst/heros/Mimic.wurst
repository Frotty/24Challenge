package Mimic

import Hero
import MirrorRocket

constant int MIRROR_ROCKET_ID = 'A000'

public class Mimic extends Hero

	construct(vec3 pos, unit u)
		super(pos, u)
		
	override function onCast(int id, vec2 tpos, unit target)
		switch id
			case MIRROR_ROCKET_ID
				print("Casted")
				new MirrorRocket(pos+ vec3(0,0,50), 20., actor, 8., 60., actor.getFacingAngle()+ angle(3.14), "Abilities\\Weapons\\IllidanMissile\\IllidanMissile.mdl")
			default
				print("not casted")
		